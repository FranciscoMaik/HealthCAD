{% extends 'layout.twig' %}

{% block body %}
  <div class="cadPac">
    <h2>Cadastrar Médico</h2>
    <form class="cadpacientes" action="cadastrodepacientes.html" method="post">
      <label for="">Nome do Paciênte</label>
      <br>
      <input type="text" name="nome" value="" placeholder="Nome Completo">
      <br>
      <label for="">CRM</label>
      <br>
      <input type="text" name="cpf" value="" placeholder="CRM">
      <br>
      <label for="">Especialidade (s)</label>
      <br>
      <select name="select">
        <option value="valor0" selected> -- Selecionar -- </option>
        <option value="valor1">Especialidade 1</option>
        <option value="valor2">Especialidade 2</option>
        <option value="valor3">Especialidade 3</option>
      </select>
      <br>
      <br>
      <button type="submit" name="enviar">Cadastrar</button>

    </form>
  </div>
{% endblock %}
